#include <stdio.h>
#include <stdlib.h>

/* typedef toplama iþlemi */

   typedef int d1;


   int main() {
	
	int toplam;
	d1 d2;       // deger1den deger2 isminde bir deðiþken türetelim
	d1 d3;       // deger1den deger3 isminde bir deðiþken türetelim
	
	d2=20;
	d3=40;
	toplam=d2+d3;
	
	printf("%d",toplam);
	
	
	
	
	return 0;
}
