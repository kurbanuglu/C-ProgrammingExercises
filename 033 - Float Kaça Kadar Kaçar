#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */
//PROBLEM NERDE!!!!!//
int main() {
	
	float a,b,i;
	
	printf("kacar kacar: ");
	scanf("%f,&a");
	
	printf("kaca kadar: ");
	scanf("%f",&b);
	
	printf("\n");
	for(i=0;i<=b;i+=a)
	{
		printf("%2f\n",i);
	}
	getch();
	
	
	return 0;
}
