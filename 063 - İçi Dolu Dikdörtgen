#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main() {
	
	int en,boy,i,j;
	
	printf("En Degeri: ");
	scanf("%d",&en);
	
	printf("Boy Degeri: ");
	scanf("%d",&boy);
	
	for(i=1;i<=boy;i++)
	{
		for(j=1;j<=en;j++)
		{
		printf("*");	
		}
	printf("\n");
	
	}
	
	
	
	return 0;
}
