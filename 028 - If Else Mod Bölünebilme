#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */
/*sayi hem 3e hem5e bolunuyor mu?*/
 
int main() {
	
	int sayi;
	
	printf("sayi giriniz: ");
	scanf("%d",&sayi);
	
	if(sayi%5==0 && sayi%3==0)
	{
		printf("sayi 3 ve 5e bolunur");
	}
	else
	{
		printf("sayi 3 ve 5e bolunemez");
	}
	
	
	
	
	
	return 0;
}
