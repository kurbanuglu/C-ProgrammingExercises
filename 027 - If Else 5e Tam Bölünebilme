#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */
//klavyeden girilen sayi 5e tam bolunuyor mu bolunmuyor mu//

int main() {
	
	int sayi;
	
	printf("sayi giriniz:");
	scanf("%d",&sayi);
	
	if(sayi%5==0)
	{
		printf("5e tam bolunur");
	}
	else
	{
		printf("5e tam bolunmez");
	}
	
	
	
	
	
	return 0;
}
