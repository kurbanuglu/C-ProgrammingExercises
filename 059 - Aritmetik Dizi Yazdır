#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main() {
	
	int dizi[100];
	int i,sayi;
	
	printf("Eleman Sayisi: ");
	scanf("%d",&sayi);
	
	for(i=0;i<sayi;i++)
	{
		printf("Dizinin %d. degerini giriniz: ", i+1 );
		scanf("%d",&dizi[i]);
	}	
	
	for(i=0;i<sayi;i++)
	{
		printf("%d ",dizi[i]);
	}
	
	
	return 0;
}
