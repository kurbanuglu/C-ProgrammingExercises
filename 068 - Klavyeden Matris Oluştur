#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main() {
	
	int satir,sutun,i,j,k,l;
	
	printf("Satir Sayisi Giriniz:");
	scanf("%d",&satir);
	
	printf("Sutun Sayisi Giriniz:");
	scanf("%d",&sutun);
	
	int matris[satir][sutun];
	
	for(i=0;i<satir;i++)
	{
		for(j=0;j<sutun;j++)
		{
			printf("\nMatris Degeriniz:[%d][%d] ---> ",i+1,j+1);
			scanf("%d",&matris[i][j]);
		}	
	}
	printf("\n\nOLUSAN MATRIS\n");
	
	for(k=0;k<satir;k++)
		{
			for(l=0;l<sutun;l++)
			{
				printf(" %d ",matris[k][l]);
			}
			printf("\n");
		}
	
	
	
	return 0;
}
