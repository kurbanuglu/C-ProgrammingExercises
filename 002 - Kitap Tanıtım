#include <stdio.h>
#include <stdlib.h>



int main() {
	
	
	char kitapad[21]="Kendini Arayan At";
	char yazar[30]="Beyza Subasi";
	char turu[10]="Biyografi";
	char sayfa[3]="100";
	char basimyil[4]="1999";
	
	printf("***** Kitap Tanitim *****\n\n\n");
	printf("Kitapad: %s - KitapYazar: %s\n",kitapad,yazar);
	printf("Kitap Turu: %s\n",turu);
	printf("Kitap Sayfa Sayisi: %s\n",sayfa);
	printf("Kitap Basim Yili: %s\n",basimyil);
	
	return 0;
}
