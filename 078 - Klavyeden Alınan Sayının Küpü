#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */
// klavyeden alýnan sayýnýn küpünü bulma //

int main() {
	
	int sayi,kup;
	
	
	printf("Sayi Giriniz: ");
	scanf("%d",&sayi);
	
	kup=sayi*sayi*sayi;
	
	printf("Sayinin Kupu: %d ", kup);
	
	

	
	return 0;
}
